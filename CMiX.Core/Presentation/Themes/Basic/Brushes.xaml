<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="SuperHighDarkerColor" Color="{DynamicResource SuperHighDarker}" />
    <SolidColorBrush x:Key="HighHighDarkerColor" Color="{DynamicResource HighHighDarker}" />
    <SolidColorBrush x:Key="HighDarkerColor" Color="{DynamicResource HighDarker}" />
    <SolidColorBrush x:Key="DarkerColor" Color="{DynamicResource Darker}" />
    <SolidColorBrush x:Key="DarkColor" Color="{DynamicResource Dark}" />
    <SolidColorBrush x:Key="Color" Color="{DynamicResource Normal}" />
    <SolidColorBrush x:Key="LightColor" Color="{DynamicResource Light}" />
    <SolidColorBrush x:Key="HighLightColor" Color="{DynamicResource HighLight}" />
    <SolidColorBrush x:Key="SuperHighLightColor" Color="{DynamicResource SuperHighLight}" />
    
    <SolidColorBrush x:Key="OverColor" Color="{DynamicResource Over}" />
    <SolidColorBrush x:Key="ActiveColor" Color="{DynamicResource Active}" />
    <SolidColorBrush x:Key="TextBase" Color="{DynamicResource Text}" />
    <SolidColorBrush x:Key="OverTextBase" Color="{DynamicResource OverText}" />
    <SolidColorBrush x:Key="ActiveTextBase" Color="{DynamicResource ActiveText}" />

    <SolidColorBrush x:Key="AccentLight1" Color="{DynamicResource AccentColorLight1}" />
    <SolidColorBrush x:Key="Accent" Color="{DynamicResource AccentColor}" />
    <SolidColorBrush x:Key="AccentDark" Color="{DynamicResource AccentColorDark1}" />
</ResourceDictionary>